                        <!-- form -->
                        <form autocomplete="off" action="/donate/registeration-step1" method="post" class="login-form">
                            <div class="row ">
                                <div class="col-lg-12">
                                    <div class="input-form">
                                        <input type="text" name="name" value="<%=full_name%>" placeholder="Full Name (required)" pattern="^[a-zA-Z]+(\s[a-zA-Z]+)?$" title="Name only contains Alphabets and blank space" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row ">
                                <div class="col-lg-12">
                                    <div class="input-form">
                                        <input type="text" name="phone" value="<%=phone_number%>" placeholder="Phone Number (required)"  pattern="[1-9]{1}[0-9]{9}" title="Must contain 10 digits 0-9 , first digit can't be 0" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-lg-12">
                                    <div class="input-form">
                                        <input type="email" name="email" value="<%=email%>" placeholder="Email ID">
                                    </div>
                                </div>
                            </div>
                            <div class="row d-flex justify-content-start">
                                <div class="col-xs-6">
                                  <div class="label-form">
                                      <label for="blood-type">Blood Group</label>
																			<p>(required)</p>
                                  </div>
                                </div>
                                <div class="col-xs-6">
                                    <div class="select-form">
                                        <select name="blood_type" required>
                                            <option id="A+" value="A+" selected>A+</option>
                                            <option id="A-" value="A-">A-</option>
                                            <option id="B+" value="B+">B+</option>
                                            <option id="B-" value="B-">B-</option>
                                            <option id="AB+" value="AB+">AB+</option>
                                            <option id="AB-" value="AB-">AB-</option>
                                            <option id="O+" value="O+">O+</option>
                                            <option id="O-" value="O-">O-</option>
                                        </select>
                                    </div>
                                </div>
                              </div>
                              <script>document.getElementById("<%=blood_group%>").selected = "true"</script>
                              <div class="row d-flex justify-content-start">
																<div class="col-xs-6">
																	<div class="label-form">
																			<label for="gender">Gender</label>
																		  <p>(required)</p>
																	</div>
																</div>
                                <div class="col-xs-6">
                                  <div class="radio-wrapper">
                                      <div class="select-radio">
                                          <div class="radio">
                                              <input id="male" name="gender" type="radio" value="male">
                                              <label for="radio-1" class="radio-label">Male</label>
                                          </div>
                                          <div class="radio">
                                              <input id="female" name="gender" type="radio" value="female">
                                              <label for="radio-2" class="radio-label">Female</label>
                                          </div>
                                      </div>
                                   </div>
                                 </div>
                              </div>
                              <script>document.getElementById("<%=gender%>").checked = "true"</script>
                            <div class="row">
                                <div class="col-sm-6">
                                  <div class="label-form">
                                      <label>Date of Birth</label>
                                  </div>
                                </div>
                                <div class="col-sm-6">
                                    <div class="input-form">
										<% let max_date = new Date();
										   max_date = (max_date.getFullYear()-12)+'-'+('0'+(max_date.getMonth()+1)).slice(-2)+'-'+('0'+(max_date.getDate())).slice(-2);
										%>
										<%var tdob = new Date(dob);%>
										<input type="date" name="dob" value="<%=tdob.getFullYear()%>-<%=('0'+(tdob.getMonth()+1)).slice(-2)%>-<%=('0'+(tdob.getDate())).slice(-2)%>" max="<%=max_date%>">
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <!-- Password -->
                                <div class="col-12">
                                    <div class="input-form">
                                        <input type="text" name="height"placeholder="Height ( in cms )"  pattern="[1-9]{1}[0-9]+" title="Must digits 0-9 , first digit can't be 0" required>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="input-form">
                                        <input type="text" name="weight" placeholder="Weight ( in kgs )" pattern="[1-9]{1}[0-9]+" title="Must digits 0-9 , first digit can't be 0" required>
                                    </div>
                                </div>
                            </div>
                            <div class="row">
                                <!-- Button -->
                                <div class="container">
                                  <div class="row justify-content-center">
                                      <div class="col-lg-4 col-md-12 col-xs-12">
                                          <button type="submit" class="btn header-btn">Register</button>
                                      </div>
                                  </div>
                                </div>
                                <!-- Button-End -->
                            </div>
                        </form>
                    </div>
                </div>
            </div>
