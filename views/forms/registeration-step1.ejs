                    <div class="login-form-wrapper">
                        <!-- From title -->
                        <div class="row">
                            <div class="col-lg-12">
                                <!-- Section Tittle -->
                                <div class="section-tittle mb-50">
                                    <h2>Registeration</h2>
                                </div>
