		<section class="login-form-area section-bg  pt-35 pb-120 fix">
			<div class="container">
				<div class="row justify-content-center">
					<!-- login wrapper -->
					<div class="col-xl-8 col-lg-9">
						<div class="login-form-wrapper">
							<!-- From title -->
							<div class="row">
								<div class="col-lg-12">
									<!-- Section Tittle -->
									<div class="section-tittle mb-50">
										<h2>Pre-Examination</h2>
									</div>
								</div>
							</div>
							<!-- form -->
							<form action="/donate/" method="post" class="login-form">

								<div class="row d-flex justify-content-start">
									<div class="col-xs-6">
										<div class="label-form">
											<label for="blood-type">Blood Group</label>
										</div>
									</div>
									<div class="col-xs-6">
										<div class="select-form">
											<select name="blood-type" required>
												<option value="" selected>A+</option>
												<!-- It should show the Blood group mentioned by Donor ( If mentioned ) -->
												<option value="">A-</option>
												<option value="">B+</option>
												<option value="">B-</option>
												<option value="">AB+</option>
												<option value="">AB-</option>
												<option value="">O+</option>
												<option value="">O-</option>
											</select>
										</div>
									</div>
								</div>
								<div class="row d-flex justify-content-start">
									<div class="col-xs-6">
										<div class="label-form">
											<label for="hg_level">Haemoglobin Level</label>
										</div>
									</div>
									<div class="col-xs-6">
										<div class="radio-wrapper">
											<div class="select-radio">
												<div class="radio">
													<input id="radio-1" name="hg_level" type="radio" value="normal">
													<label for="radio-1" class="radio-label">Normal</label>
												</div>
												<div class="radio">
													<input id="radio-2" name="hg_level" type="radio" value="not_normal">
													<label for="radio-2" class="radio-label">Not Normal</label>
												</div>
											</div>
										</div>
									</div>
								</div>

								<div class="row d-flex justify-content-start">
									<div class="col-xs-6">
										<div class="label-form">
											<label for="bp_level">Blood Pressure&emsp;&ensp;</label>
										</div>
									</div>
									<div class="col-xs-6">
										<div class="radio-wrapper">
											<div class="select-radio">
												<div class="radio">
													<input id="radio-3" name="bp_level" type="radio" value="normal">
													<label for="radio-3" class="radio-label">Normal</label>
												</div>
												<div class="radio">
													<input id="radio-4" name="bp_level" type="radio" value="not_normal">
													<label for="radio-4" class="radio-label">Not Normal</label>
												</div>
											</div>
										</div>
									</div>
								</div>

								<div class="row d-flex justify-content-start">
									<div class="col-xs-6">
										<div class="label-form">
											<label for="temp_level">Temperature Level</label>
										</div>
									</div>
									<div class="col-xs-6">
										<div class="radio-wrapper">
											<div class="select-radio">
												<div class="radio">
													<input id="radio-5" name="temp_level" type="radio" value="normal">
													<label for="radio-5" class="radio-label">Normal</label>
												</div>
												<div class="radio">
													<input id="radio-6" name="temp_level" type="radio" value="not_normal">
													<label for="radio-6" class="radio-label">Not Normal</label>
												</div>
											</div>
										</div>
									</div>
								</div>

								<div class="row d-flex justify-content-start">
									<div class="col-xs-6">
										<div class="label-form">
											<label for="pulse">Pulse Rate&emsp;&emsp;&emsp;&ensp;</label>
										</div>
									</div>
									<div class="col-xs-6">
										<div class="radio-wrapper">
											<div class="select-radio">
												<div class="radio">
													<input id="radio-7" name="pulse" type="radio" value="normal">
													<label for="radio-7" class="radio-label">Normal</label>
												</div>
												<div class="radio">
													<input id="radio-8" name="pulse" type="radio" value="not_normal">
													<label for="radio-8" class="radio-label">Not Normal</label>
												</div>
											</div>
										</div>
									</div>
								</div>

								<!-- Button -->
								<div class="container">
									<div class="row justify-content-center">
										<div class="col-lg-4 col-md-12 col-xs-12">
											<button type="submit" class="btn header-btn">Verify</button>
										</div>
									</div>
								</div>
								<!-- Button-End -->
							</div>
						</form>
					</div>
				</div>
